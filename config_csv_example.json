{
    "url": "https://api.example.com/v1/chat/completions",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "Bearer sk-xxxxxxxxxxxxxxxx"
    },
    "data_source": {
      "type": "csv",
      "file": "test_data.csv",
      "column": "content",
      "encoding": "utf-8"
    },
    "body": {
      "model": "gpt-3.5-turbo",
      "messages": [
        {
          "role": "user",
          "content": "{{content}}"
        }
      ],
      "stream": true,
      "max_tokens": 500,
      "temperature": 0.7
    },
    "timeout": 300,
    "concurrency": 10,
    "total_requests": 100
  }
